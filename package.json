{
  "version": "0.0.6",
  "name": "navigatio",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreyselin/navigatio"
  },
  "license": "MIT",
  "packageManager": "yarn@3.6.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./navigatio.css": "./dist/navigatio.css"
  },
  "files": [
    "dist",
    "types",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "build": "rm -rf dist && rm -rf types && vite build && tsc -p tsconfig.build.json"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "sass-embedded": "^1.93.2",
    "scss": "^0.2.4",
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "npm:rolldown-vite@7.1.12"
  },
  "resolutions": {
    "vite": "npm:rolldown-vite@7.1.12"
  },
  "sideEffects": ["**/*.css", "**/*.scss"]
}
